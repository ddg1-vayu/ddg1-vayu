{
	"[blade]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.insertSpaces": false,
		"editor.tabSize": 2
	},
	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.insertSpaces": false,
		"editor.tabSize": 2
	},
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.insertSpaces": false,
		"editor.tabSize": 2
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.insertSpaces": false,
		"editor.tabSize": 2
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features",
		"editor.insertSpaces": false,
		"editor.tabSize": 2
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features",
		"editor.insertSpaces": false,
		"editor.tabSize": 2
	},
	"[php]": {
		"editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
		"editor.insertSpaces": false,
		"editor.tabSize": 2
	},
	"[python]": {
		"editor.formatOnSave": true,
		"editor.formatOnType": true,
		"editor.insertSpaces": true,
		"editor.tabSize": 4
	},
	"[sql]": {
		"editor.defaultFormatter": "dbcode.dbcode"
	},
	"[xml]": {
		"editor.defaultFormatter": "DotJoshJohnson.xml"
	},
	"auto-close-tag.activationOnLanguage": [
		"xml",
		"php",
		"blade",
		"ejs",
		"jinja",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"plaintext",
		"markdown",
		"vue",
		"liquid",
		"erb",
		"lang-cfml",
		"cfml",
		"HTML (EEx)",
		"HTML (Eex)",
		"plist"
	],
	"chat.agent.enabled": true,
	"chat.agent.thinkingStyle": "collapsedPreview",
	"chat.editor.fontFamily": "Monaspace Neon Frozen",
	"chat.editor.fontSize": 13,
	"chat.editor.wordWrap": "on",
	"chat.emptyState.history.enabled": true,
	"chat.math.enabled": true,
	"clock.alignment": "Right",
	"clock.format": "dddd | dd mmm yyyy | hh:MM:ss TT",
	"code-runner.executorMap": {
		"php": "C:/xampp/php/php.exe",
		"python": "C://Progra~1//Python313//python.exe",
		"javascript": "node"
	},
	"code-runner.showRunCommandInExplorerContextMenu": false,
	"colorize.languages": [
		"css",
		"html",
		"javascript",
		"json",
		"jsonc",
		"php"
	],
	"cSpell.allowCompoundWords": true,
	"cSpell.enabledFileTypes": {
		"*": true,
		"jsonc": false,
		"xml": false
	},
	"cSpell.language": "en",
	"dbcode.ai.inlineCompletion": false,
	"dbcode.history.enabled": true,
	"dbcode.resultLocation": "beside",
	"diffEditor.codeLens": true,
	"diffEditor.experimental.showMoves": true,
	"diffEditor.ignoreTrimWhitespace": true,
	"diffEditor.maxComputationTime": 0,
	"editor.accessibilitySupport": "off",
	"editor.aiStats.enabled": true,
	"editor.bracketPairColorization.enabled": true,
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"editor.cursorBlinking": "smooth",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.find.addExtraSpaceOnTop": false,
	"editor.fontFamily": "Monaspace Neon Frozen",
	"editor.fontLigatures": "'zero', 'liga', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09', 'ss10', 'calt'",
	"editor.fontSize": 15,
	"editor.formatOnType": true,
	"editor.guides.bracketPairs": "active",
	"editor.guides.indentation": true,
	"editor.inlayHints.enabled": "on",
	"editor.inlineSuggest.fontFamily": "Monaspace Neon Frozen",
	"editor.insertSpaces": false,
	"editor.letterSpacing": -0.5,
	"editor.lineHeight": 1.5,
	"editor.linkedEditing": true,
	"editor.minimap.enabled": false,
