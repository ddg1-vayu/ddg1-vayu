{
	"[blade]": {
		"editor.defaultFormatter": "shufo.vscode-blade-formatter",
		"editor.insertSpaces": false,
	},
	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.insertSpaces": false,
	},
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features",
		"editor.insertSpaces": false,
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.insertSpaces": false,
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features",
		"editor.insertSpaces": false,
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features",
		"editor.insertSpaces": false,
	},
	"[php]": {
		"editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
		"editor.insertSpaces": false,
		"editor.tabSize": 2
	},
	"[python]": {
		// "editor.defaultFormatter": "ms-python.black-formatter",
		"editor.formatOnSave": true,
		"editor.formatOnType": true,
		"editor.insertSpaces": true,
		"editor.tabSize": 4
	},
	"[sql]": {
		"editor.defaultFormatter": "dbcode.dbcode"
	},
	"[xml]": {
		"editor.defaultFormatter": "DotJoshJohnson.xml"
	},
	"auto-close-tag.activationOnLanguage": [
		"xml",
		"php",
		"blade",
		"ejs",
		"jinja",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"plaintext",
		"markdown",
		"vue",
		"liquid",
		"erb",
		"lang-cfml",
		"cfml",
		"HTML (EEx)",
		"HTML (Eex)",
		"plist"
	],
	"cSpell.allowCompoundWords": true,
	"cSpell.enabledFileTypes": {
		"*": true,
		"jsonc": false,
		"xml": false
	},
	"cSpell.ignoreWords": [
		"undislike"
	],
	"cSpell.language": "en",
	"cSpell.userWords": [
		"behavioural",
		"dotenv",
		"Dugar",
		"hbspt",
		"hsapi",
		"hubspotutk",
		"kaleyra",
		"Kolkata",
		"Photoshop",
		"rediffmail",
		"Vayu"
	],
	"clock.alignment": "Right",
	// "clock.format": "dddd | dd mmm yyyy | hh:MM:ss TT",
	"clock.format": "dddd | dd mmm yyyy",
	"code-runner.executorMap": {
		"php": "C:/xampp/php/php.exe",
		"python": "C://Progra~1//Python312//python.exe",
		"javascript": "node"
	},
	"code-runner.showRunCommandInExplorerContextMenu": false,
	"colorize.languages": [
		"css",
		"html",
		"javascript",
		"json",
		"jsonc",
		"php"
	],
	"diffEditor.experimental.showMoves": true,
	"diffEditor.ignoreTrimWhitespace": true,
	"diffEditor.maxComputationTime": 0,
	"editor.accessibilitySupport": "off",
	"editor.bracketPairColorization.enabled": true,
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"editor.cursorBlinking": "smooth",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.find.addExtraSpaceOnTop": false,
	"editor.fontFamily": "Fantasque Sans Mono",
	// "editor.fontFamily": "Inter Tight", //Fira Code iScript || Fantasque Sans Mono
	"editor.fontLigatures": "'zero', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'calt'",
	"editor.fontSize": 16,
	"editor.formatOnType": true,
	"editor.guides.bracketPairs": "active",
	"editor.guides.indentation": true,
	"editor.inlayHints.enabled": "on",
	"editor.insertSpaces": false,
	"editor.lineHeight": 1.5,
	"editor.linkedEditing": true,
	"editor.minimap.enabled": false,
	"editor.minimap.maxColumn": 150,
	"editor.minimap.renderCharacters": false,
	"editor.renderWhitespace": "none",
	"editor.scrollBeyondLastLine": false,
	"editor.snippetSuggestions": "inline",
	"editor.stickyScroll.enabled": false,
	"editor.stickyScroll.maxLineCount": 4,
	"editor.suggestSelection": "first",
	"editor.tabCompletion": "onlySnippets",
